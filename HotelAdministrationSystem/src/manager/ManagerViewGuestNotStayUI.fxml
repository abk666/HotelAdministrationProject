<AnchorPane prefHeight="650.0" prefWidth="950.0" style="-fx-background-color: #d1e0ea;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="manager.ManagerViewGuestNotStayController">
