<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="650.0" prefWidth="950.0" style="-fx-background-color: #d1e0ea;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="manager.ManagerViewGuestController">
   <children>
      <Label alignment="CENTER" layoutX="1.0" prefHeight="60.0" prefWidth="994.0" text="Guest" textFill="#114ca0">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <TableView fx:id="staffTable" layoutX="-1.0" layoutY="178.0" prefHeight="472.0" prefWidth="1002.0">
        <columns>
          <TableColumn fx:id="staffId" prefWidth="46.0" text="Id" />
          <TableColumn fx:id="staffUserName" prefWidth="114.0" text="Name" />
            <TableColumn fx:id="staffEmail" prefWidth="164.0" text="Email" />
            <TableColumn fx:id="staffPhNo" prefWidth="121.0" text="Phone" />
            <TableColumn fx:id="staffAddress" prefWidth="119.0" text="Address" />
            <TableColumn fx:id="staffDOB" prefWidth="111.0" text="Date of Birth" />
            <TableColumn fx:id="staffGender" prefWidth="105.0" text="Gender" />
            <TableColumn fx:id="staffRole" prefWidth="101.0" text="Role" />
            <TableColumn fx:id="staffImageName" prefWidth="120.0" text="Image" />
        </columns>
      </TableView>
      <JFXComboBox fx:id="cobSearch" focusColor="#0363aa" layoutX="53.0" layoutY="104.0" prefHeight="35.0" prefWidth="187.0" unFocusColor="#113c9e" />
      <JFXTextField fx:id="tfSearch" focusColor="#0363aa" layoutX="240.0" layoutY="104.0" prefHeight="35.0" prefWidth="187.0" unFocusColor="#113c9e" />
      <ImageView fitHeight="60.0" fitWidth="60.0" layoutX="1.0" layoutY="3.0" onMouseClicked="#processBack" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/previous.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="50.0" fitWidth="50.0" layoutX="427.0" layoutY="90.0" onMouseClicked="#processSearch" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/search.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="50.0" fitWidth="50.0" layoutX="488.0" layoutY="90.0" onMouseClicked="#processRefresh" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/refresh%20(2).png" />
         </image>
      </ImageView>
      <ImageView fitHeight="60.0" fitWidth="60.0" layoutX="835.0" layoutY="90.0" onMouseClicked="#processStay" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/quarantine.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="90.0" fitWidth="90.0" layoutX="900.0" layoutY="70.0" onMouseClicked="#processNotStay" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/isolated.png" />
         </image>
      </ImageView>
   </children>
